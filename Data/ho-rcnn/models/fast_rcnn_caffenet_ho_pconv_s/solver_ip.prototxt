train_net: "models/fast_rcnn_caffenet_ho_pconv_s/train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 100000
display: 20
# average_loss has no effect in the current implementation
# average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "fast_rcnn_caffenet_pconv_ip"
# debug_info: true
